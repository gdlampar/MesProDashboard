{
  "type": "object",
  "properties": {
    "ishome": {
      "type": "identifier",
      "selector": ".gauge",
      "control": "exists"
    },
    "total": {
      "type": "string",
      "control": "property.innerHTML",
      "selector": ".Total .badge"
    },
    "idle": {
      "type": "string",
      "selector": ".Idle .badge",
      "control": "property.innerText"
    },
    "down": {
      "type": "string",
      "selector": ".Down .badge",
      "control": "property.innerHTML"
    },
    "outofcycle": {
      "type": "string",
      "selector": ".OverTime .badge",
      "control": "property.innerHTML"
    },
    "unknown": {
      "type": "string",
      "selector": ".UnknownCycle .badge",
      "control": "property.innerHTML"
    },
    "trailrun": {
      "type": "string",
      "selector": ".TrailRun .badge",
      "control": "property.innerHTML"
    },
    "setup": {
      "type": "string",
      "selector": ".Setup .badge",
      "control": "property.innerHTML"
    },
    "processing": {
      "type": "string",
      "selector": ".Processing .badge",
      "control": "property.innerHTML"
    },
    "maintenance": {
      "type": "string",
      "selector": ".Maintenance .badge",
      "control": "property.innerHTML"
    },
    "break": {
      "type": "string",
      "selector": ".Break .badge",
      "control": "property.innerHTML"
    },
    "action3": {
      "type": "action",
      "selector": "a",
      "selectContains": "OEE DETAIL",
      "control": "action.link"
    }
  },
  "connect": [
    {
      "match": [
        "ishome"
      ],
      "screens": [
        "home"
      ]
    },
    {
      "match": [],
      "action": "action3",
      "model": "oeedetail"
    }
  ]
}
