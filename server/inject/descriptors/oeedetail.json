{
  "type": "object",
  "properties": {
    "isoeedetail": {
      "type": "string",
      "selector": "head > title",
      "control": "exists",
      "selectContains": "OEE Detail - MESPRO Dashboard",
      "selectHidden": true
    },
    "listItems": {
      "type": "array",
      "selectorAll": "#oee-table > tbody > tr",
      "items": {
        "type": "object",
        "properties": {
          "machine": {
            "type": "string",
            "control": "property.innerText",
            "selector": "td"
          },
          "ava": {
            "type": "string",
            "selector": "td:nth-child(3)",
            "control": "property.innerText"
          },
          "per": {
            "type": "string",
            "selector": "td:nth-child(4)",
            "control": "property.innerText"
          },
          "yield": {
            "type": "string",
            "selector": "td:nth-child(y)",
            "control": "property.innerText"
          },
          "oee": {
            "type": "string",
            "selector": "td:nth-child(6)",
            "control": "property.innerText"
          }
        }
      }
    }
  },
  "connect": []
}
